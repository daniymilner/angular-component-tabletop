div(ng-controller='tableCtrl as table')
	md-table-container
		table.md-cell-select.multiple.md-table()
			thead.md-head
				tr.md-row
					th
					th.md-column(ng-repeat='version in table.versions', ng-bind='version.name')
			tbody.md-body
				tr.md-row(ng-repeat='company in table.companies')
					td.md-cell(ng-bind='company')
					td.md-cell(ng-repeat='version in table.versions')
